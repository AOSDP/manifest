<?xml version="1.0" encoding="UTF-8"?>
<manifest>

  <remote  name="github"
           fetch="https://github.com" />
  
  <remote  name="aosdp"
           fetch="https://github.com/AOSDP"
           revision="refs/heads/Oatcake" />

  <!-- AOSDP -->
  <project path="manifest" name="manifest" remote="aosdp" />
  <project path="build/make" name="android_build" remote="aosdp" >
    <copyfile src="core/root.mk" dest="Makefile" />
    <linkfile src="CleanSpec.mk" dest="build/CleanSpec.mk" />
    <linkfile src="buildspec.mk.default" dest="build/buildspec.mk.default" />
    <linkfile src="core" dest="build/core" />
    <linkfile src="envsetup.sh" dest="build/envsetup.sh" />
    <linkfile src="target" dest="build/target" />
    <linkfile src="tools" dest="build/tools" />
  </project>
  <project path="frameworks/av" name="android_frameworks_av" groups="pdk" remote="aosdp" />
  <project path="frameworks/base" name="android_frameworks_base" remote="aosdp" />
  <project path="frameworks/native" name="android_frameworks_native" groups="pdk" remote="aosdp" />
  <project path="frameworks/opt/telephony" name="android_frameworks_opt_telephony" groups="pdk" remote="aosdp" />
  <project path="system/libvintf" name="android_system_libvintf" groups="pdk" />
  <project path="system/bt" name="android_system_bt" groups="pdk" remote="aosdp" />
  <project path="system/core" name="android_system_core" groups="pdk" remote="aosdp" />
  <project path="system/netd" name="android_system_netd" groups="pdk" remote="aosdp" />
  <project path="system/vold" name="android_system_vold" groups="pdk" remote="aosdp" />

</manifest> 
